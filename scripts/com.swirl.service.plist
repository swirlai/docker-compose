<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.swirl.service</string>

    <key>ProgramArguments</key>
    <array>
        <string>{{SWIRL_SCRIPT_PATH}}</string>
    </array>

    <key>KeepAlive</key>
    <false/>

    <key>RunAtLoad</key>
    <true/>

    <key>StandardOutPath</key>
    <string>{{HOME_LOG_PATH}}/tmp/log/swirl-service.out</string>

    <key>StandardErrorPath</key>
    <string>{{HOME_LOG_PATH}}/tmp/log/swirl-service.err</string>
</dict>
</plist>
